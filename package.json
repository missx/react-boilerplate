    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
